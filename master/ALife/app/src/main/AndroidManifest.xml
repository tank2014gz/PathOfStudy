<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.db.alife" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fog_material"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".drawer.SettingsActivity"
            android:theme="@style/nLiveoDrawer" >
        </activity>
        <activity
            android:name=".drawer.MainActivity"
            android:label="ALife"
            android:theme="@style/nLiveoDrawer" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MotoDetailsActivity"
            android:label="@string/title_activity_moto_details"
            android:theme="@style/nLiveoDrawer" >
        </activity>
        <activity
            android:name=".activity.ReadDetailsActivity"
            android:label="@string/title_activity_read_details"
            android:theme="@style/nLiveoDrawer" >
        </activity>
        <activity
            android:name=".activity.SentenceDetailsActivity"
            android:label="@string/title_activity_sentence_details"
            android:theme="@style/nLiveoDrawer" >
        </activity>
        <activity
            android:name=".activity.WorldDetailsActivity"
            android:label="@string/title_activity_world_details"
            android:theme="@style/nLiveoDrawer" >
        </activity>
    </application>

</manifest>
